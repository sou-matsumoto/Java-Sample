package sampleP4;

public class Sample4 {

	public static void main(String[] args) 
	{
		int num1=10;
		int num2=5;
		
		System.out.println("num1とnum2にいろんな計算をします。");
		System.out.println("num1+num2は"+(num1+num2)+"です。");
		System.out.println("num1-num2は"+(num1-num2)+"です。");
		System.out.println("num1*num2は"+(num1*num2)+"です。");
		System.out.println("num1/num2は"+(num1/num2)+"です。");
		System.out.println("num1%num2は"+(num1%num2)+"です。");

	}

}
