package sampleP7;

public class Sample10 {

	public static void main(String[] args) 
	{
		int [][] test=new int[2][5];

		test[0][0]=80;
		test[0][1]=70;
		test[0][2]=60;
		test[0][3]=65;
		test[0][4]=25;
		test[1][0]=55;
		test[1][1]=30;
		test[1][2]=75;
		test[1][3]=85;
		test[1][4]=95;
		
		for(int i=0; i<5; i++){
			System.out.println((i+1)+"番目の国語の点数は"+test[0][i]+"点です。");
			System.out.println((i+1)+"番目の算数の点数は"+test[1][i]+"点です。");
		}
	
	}

}
