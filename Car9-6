package sampleP9;

	class Car6 
	{
		private int num;
		private double gas;
		
		public Car6()
		{
			num=0;
			gas=0.0;
			
			System.out.println("車を作成しました。");
		}
		public Car6(int n,double g)
		{
			num=n;
			gas=g;
			
			System.out.println("車のナンバーを"+num+"にガソリン量を"+gas+"に変更しました。");
		}
		public void show()
		{
			System.out.println("車のナンバーは"+num+"です。");
			System.out.println("ガソリン量は"+gas+"です。");
		}

}
