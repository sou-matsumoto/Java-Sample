package sample10;

public class Sample9 {

	public static void main(String[] args)
	{
		Car9[] cars;
		cars=new Car9[3];

		for(int i=0; i<cars.length; i++)
		cars[i]=new Car9();
		{
			cars[0].setCar(1234, 20.5);
			System.out.println();
			cars[1].setCar(2345, 30.5);
			System.out.println();
			cars[2].setCar(3456, 40.5);
			System.out.println();
		}
		for(int i=0; i<cars.length; i++)
		cars[i].show();
		{
		}

	}

}
